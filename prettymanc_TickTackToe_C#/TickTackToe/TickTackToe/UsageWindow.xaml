<Window x:Class="TickTackToe.UsageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UsageWindow" Height="300" Width="300" ResizeMode="NoResize" Foreground="#FF1D0000" WindowStartupLocation="CenterScreen" Icon="ttticon.ico">
    <Grid Background="#FFBE451C">
        <Label x:Name="lblUsageInfo" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0" Foreground="#FF1D0000"/>
        <Button x:Name="btnOk" Content="Ok" HorizontalAlignment="Center" Width="75" Margin="0,0,0,10" Foreground="#FF1D0000" Background="#FF912F0D" Click="btnOk_Click" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
